---
    title: My NFTs
    layout: layouts/base.njk
---

<div class="header">
    <h1>{{ title }}</h1>
</div>

<div class="nft-collection">
    {% for post in collections.nfts %}
        <div class="nft-entry">
            <img src="{{post.data.openSea.image_url}}">
            <a href="{{post.url}}">{{post.data.title}}</h1>
        </div>
    {%endfor%}
</div>